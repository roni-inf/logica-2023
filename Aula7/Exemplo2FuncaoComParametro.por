programa
{
	
	funcao inicio()
	{
		inteiro a, b, c
		escreva("Digite os nº","\n")
		leia(a,b,c)
		c = c + 10
		soma(a,c)
	}

	funcao soma(inteiro num1, inteiro num2){
		escreva("Soma:", num1+num2)
	}
}
